{
    // Enforcing
    "curly"    : false,
    "forin"    : false,
    "latedef"  : "nofunc",
    "newcap"   : true,
    "quotmark" : "double",
    "eqeqeq"   : true,
    "undef"    : true,
    "globals"  : {
        "logger" : false,
        "mendix" : false,
        "mx"     : false,
        "mxui"   : false
    },
    "sub"      : true,

    // Relaxing
    "laxbreak" : true,

    // Environments
    "browser"  : true,
    "devel"    : true,
    "dojo"     : true,

    // Exceptions to the rule
    "-W004"    : false,      // prevent 'i is already defined'
    "-W083"    : false       // Don't make functions in a loop
}
